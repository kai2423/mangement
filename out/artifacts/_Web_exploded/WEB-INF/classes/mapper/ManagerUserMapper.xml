<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="studentstatns.mangement.mapper.ManagerUserMapper">

    <select id="getManagementUserByIDAndPassword" resultType="studentstatns.mangement.entity.ManagementUser">
        select * from management_users where id = #{id} and passwd = #{password}
    </select>

</mapper>