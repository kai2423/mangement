<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="studentstatns.mangement.mapper.StudentUserMapper">



    <select id="getStudentUserByIDAndpassword" resultType="studentstatns.mangement.entity.StudentUser">
        select * from student_users where id = #{id} and passwd = #{password}
    </select>

</mapper>
